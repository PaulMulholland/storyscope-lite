		}
	}
	else {
		if($end) {
			$output .= 'source:             "'.$base_url.'/settingtimelinedata?location='.$location.'&time='.$time.'&end='.$end.'"';
		}
		else {
			$output .= 'source:             "'.$base_url.'/settingtimelinedata?location='.$location.'&time='.$time.'"';
		}
