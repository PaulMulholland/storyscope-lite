  /**
   * Return prefixes as a string given an array of namespaces and their abbreviations
   */
  function dec_generate_prefixes($namespaces) {
  	$prefixes = '';
  	foreach ($namespaces as $namespace) {
  		$prefixes .= 'PREFIX ' . $namespace['prefix'] . ':<' . $namespace['path'] . '> ';
  	}
  	return $prefixes;
  }
  
