// GROUP BY result.tag
// ORDER BY frequency DESC) as frequency
// WHERE frequency > 1 LIMIT ".$n." ;";
	//this version of the query excludes locations
	$query = "SELECT * FROM (SELECT result.tag as value, COUNT(result.tag) as frequency, result.name
