					$new_events = explode(',',$record->events);
					$output[$record->value] = array_unique(array_merge($new_events, $output[$record->value]));
				}
				else {
					$output[$record->value] =  array_unique(explode(',',$record->events));
				}
			}
		}
	}
	
	//if tag is itself an event
