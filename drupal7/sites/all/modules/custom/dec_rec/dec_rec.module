/**
 * Cache the data for a setting timeline
*/
function save_setting_timelime($location, $start, $end, $event_type, $timeline_data) {
	$timeline_data = my_escape_string($timeline_data);
	if($event_type) {
		$query = "INSERT INTO setting_timeline_data (location, start, end, event_type, timeline_data) VALUES (:location, :start, :end, :event_type, :data) ON DUPLICATE KEY UPDATE location = VALUES(location), start = VALUES(start), end = VALUES(end), event_type = VALUES(event_type), timeline_data = VALUES(timeline_data);";
		db_query($query, array(':location' => $location, ':start' => $start, ':end' => $end, ':event_type' => $event_type, ':data' => $timeline_data));
	}
	else {
		$query = "INSERT INTO setting_timeline_data (location, start, end, event_type, timeline_data) VALUES (:location, :start, :end, 'no_event_type', :data) ON DUPLICATE KEY UPDATE location = VALUES(location), start = VALUES(start), end = VALUES(end), event_type = VALUES(event_type), timeline_data = VALUES(timeline_data);";
		db_query($query, array(':location' => $location, ':start' => $start, ':end' => $end, ':data' => $timeline_data));
	}
}

