function check_freebase_saved_image($topic = '/m/01qwfk', $thumbnail_url = FALSE, $reload = FALSE) {
	global $base_url;
	//check in local cache for image id
	if(!$reload) {
		$image_topic_test = get_topic_image_topic($topic);
	}
	if(!empty($image_topic_test)) {
