	if(isset($_GET['attribute'])) {
		$attribute = $_GET['attribute'];
	}
	else {
		$attribute = FALSE;
	}
	//check for reload
	if(isset($_GET['reload'])) {
		$reload = $_GET['reload'];
		if($reload) {
