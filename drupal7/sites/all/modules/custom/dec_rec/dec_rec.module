function setting_count_frequency($a, $b) {
	if($a['count'] == $b['count']) {
		return 0;
	}
	return ($a['count'] > $b['count']) ? -1 : 1;
}

function setting_frequency($a, $b) {
	if($a->count == $b->count) {
		return 0;
	}
	return ($a->count > $b->count) ? -1 : 1;
}

