   */
  function dec_is_uri($string) {
  	$pos = strpos($string, "http:/");
  	if ($pos !== false && $pos == 0) {
  		return true;
  	} else {
  		$pos = strpos($string, "file:/");
  		if ($pos !== false && $pos == 0) {
  			return true;
  		} else {
  			return false;
  		}
  	}
  }
  
