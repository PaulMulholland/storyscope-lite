function get_n_matching_settings_for_a_dossier($dossier, $n = 5) {
	$query = "SELECT location_table.value AS location, tag_name.name, time_table.value AS time
	FROM event_attribute_value location_table, event_attribute_value time_table, tag_name, event_attribute_value, query_tag AS qt 
	WHERE qt.id = '".$dossier."' AND
	event_attribute_value.value = qt.tag AND
	event_attribute_value.event = location_table.event AND
	location_table.attribute = 'location' AND
	(time_table.attribute = 'start_time' OR time_table.attribute = 'end_time') AND
	location_table.event = time_table.event AND
	tag_name.tag = location_table.value AND EXISTS (SELECT query_tag.tag 
	FROM query_tag 
	WHERE query_tag.id = '".$dossier."' AND 
	(location_table.value = query_tag.tag OR 
	time_table.value = query_tag.tag)) LIMIT ".$n.";";

	$result = db_query($query);
	
	$output = array();
	foreach ($result as $record) {
		$record->time = substr($record->time,0,4);
		if(isset($output[$record->location.'-'.$record->time])) {
			$prev = $output[$record->location.'-'.$record->time];
			$record->count = $prev->count + 1;
		}
		else {
			$record->count = 1;
		}
		$output[$record->location.'-'.$record->time] = $record;
			
	}
	
	uasort($output, 'setting_frequency');
	if(count($output) > $n) {
		$output = array_slice($output, 0, $n);
	}
	
	return $output;
}


